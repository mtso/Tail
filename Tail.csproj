<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Title>Tail</Title>
    <PackageProjectUrl>https://github.com/mtso/Tail</PackageProjectUrl>
    <Description>Read contents from the end of a file. 

Uses FileStream.Seek to read a file backwards from the end.</Description>
    <Summary>Read from end of file.</Summary>
    <Authors>mtso</Authors>
    <Version>1.0.14-alpha</Version>
    <AssemblyName>Tail</AssemblyName>
    <TargetFrameworks Condition="'$(LibraryFrameworks)'==''">netstandard1.0;netstandard1.3</TargetFrameworks>
    <TargetFrameworks Condition="'$(LibraryFrameworks)'!=''">$(LibraryFrameworks)</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.IO" Version="4.3.0" />
  </ItemGroup>
</Project>